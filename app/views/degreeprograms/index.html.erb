<div class="container">
<br/>
<p id="notice"><%= notice %></p>
<% if can? :create, Degreeprogram.new %>
  <a class="btn btn-success pull-right" href="/degreeprograms/new" role="button">New Degreeprogram</a>
<% end %>
<h1 >Degree Programs</h1>
<br/>
<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>Type</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @degreeprograms.each do |degreeprogram| %>
      <tr>
        <td><%= degreeprogram.degree_type %></td>
        <td><%= degreeprogram.name %></td>
        <td><%= link_to 'Show', degreeprogram %></td>
        <td><%= link_to 'Edit', edit_degreeprogram_path(degreeprogram) %></td>
        <td><%= link_to 'Destroy', degreeprogram, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


